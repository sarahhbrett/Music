<h1> Login </h1>

<%= form_tag('/login') do %>
	<table>
	 <tr>
			<td><%= label_tag :name %></td>
			 <td><%= text_field_tag :name %>
		</tr>
		<tr>
			<td><%= label_tag :password %></td>
			<td><%= password_field_tag :password %>
		</tr>
	 <tr>
		<td><%= submit_tag "Log In" %></td>
	 </tr>
	</table>
<% end %>
	